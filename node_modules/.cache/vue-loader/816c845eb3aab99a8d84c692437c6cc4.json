en+Pro:wght@400;500;600;700;800;900&display=swap');\n.searchbox-container {\n  width: 100%;\n  height: 10vh;\n  display: flex;\n  justify-content: center;\n  margin-top: 100px;\n}\n.searchbox {\n\n}\n.results-container {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-evenly;\n  flex-wrap: wrap;\n}\n\n</style>\n"]}]}